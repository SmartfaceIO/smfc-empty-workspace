{
    ".mapView": {
        "height": 343,
        "width": 343,
        "touchEnabled": true,
        "rotateEnabled": true,
        "compassEnabled": true,
        "scrollEnabled": true,
        "type": "NORMAL",
        "visible": true,
        "backgroundColor": "#FFFFFF",
        "alpha": 1,
        "borderColor": "#000000",
        "borderWidth": 0,
		"borderRadius": 8,
        "flexProps": {
            "alignSelf": "AUTO",
            "positionType": "RELATIVE"
        },
        "&-small": {
            "width": 100
        },
        "&-medium": {
            "width": 250
        },
        "&-large": {
            "width": 350
        }
    }
}
