{
    ".sf-label": {
        "height": 22,
        "width": null,
        "touchEnabled": true,
        "visible": true,
        "multiline": false,
        "backgroundColor": "rgba(0,0,0,0)",
        "alpha": 1,
        "borderColor": "#000000",
        "borderWidth": 0,
        "textColor": "#00a1f1",
        "textAlignment": "MIDLEFT",
        "marginTop": 1,
        "marginBottom": 1,
		"font": {
			"size": 17,
			"bold": true,
			"italic": false,
			"family": "SFProText",
			"style": "Semibold"
		},
        "flexProps": {
            "alignSelf": "AUTO",
            "positionType": "RELATIVE"
        },
        "&-small": {
            "width": 100,
            "height": 16,
            "font":{
                "size": 12
            }
        },
        "&-medium": {
            "width": 190,
            "height": 30,
            "font":{
                "size": 24,
			    "style": "Medium"
            }
        },
        "&-large": {
            "width": 255,
            "height": 50,
            "font":{
                "size": 32,
			    "style": "Medium"
            }
        }
    },
    ".label": {
