{
    ".searchView": {
        "touchEnabled": true,
        "width": null,
        "height": 70,
        "visible": true,
        "alpha": 1,
        "borderColor": "#000000",
        "textColor": "#000000",
        "textFieldBackgroundColor": "rgba(142,142,147,0.12)",
        "cursorColor": "rgba(66,107,242,1)",
        "hintColor": "rgb(142,142,147)",
        "borderWidth": 0,
        "textAlignment": "MIDLEFT",
        "marginLeft": 16,
        "marginRight": 16,
        "marginTop": 16,
        "marginBottom": 16,
		"font": {
			"size": 17,
			"bold": true,
			"italic": false,
			"family": "SFProText",
			"style": "Regular"
		},
        "flexProps": {
            "alignSelf": "AUTO",
            "positionType": "RELATIVE"
        },
        "input": {
            "height": 28
        },
        "minHeight": 30,
        "&-small": {
            "width": 100
        },
        "&-medium": {
            "width": 250
        },
        "&-large": {
            "width": 350
        }
    }
}
